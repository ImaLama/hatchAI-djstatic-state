{
  "name": "claude-agents",
  "displayName": "Claude Agents",
  "description": "Tmux session integration for Claude Code agents",
  "version": "0.8.8",
  "publisher": "djhatch",
  "repository": {
    "type": "git",
    "url": "https://github.com/djhatch/claude-agents-extension"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished",
    "*"
  ],
  "main": "./out/extension_minimal.js",
  "contributes": {
    "commands": [
      {
        "command": "claude-agents.refresh",
        "title": "Refresh Tmux Sessions",
        "category": "Claude Agents"
      }
    ],
    "keybindings": [
      {
        "command": "claude-agents.refresh",
        "key": "ctrl+alt+a"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.7.4"
  }
}